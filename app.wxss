/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

@font-face {
  font-family: 'iconfont'; /* project id 924277 */
  src: url('https://at.alicdn.com/t/font_924277_y0vqk8f5am.eot');
  src: url('https://at.alicdn.com/t/font_924277_y0vqk8f5am.eot?#iefix') format('embedded-opentype'),
         url('https://at.alicdn.com/t/font_924277_y0vqk8f5am.woff') format('woff'),
         url('https://at.alicdn.com/t/font_924277_y0vqk8f5am.ttf') format('truetype'),
         url('https://at.alicdn.com/t/font_924277_y0vqk8f5am.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 24rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-chihe::after{
  content: '\e6f2';
  color: #fff;
}